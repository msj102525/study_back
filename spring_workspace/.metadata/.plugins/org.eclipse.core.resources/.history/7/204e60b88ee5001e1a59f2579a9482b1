		<result property="boardTitle" column="BOARD_TITLE" />
		<result property="boardReadCount" column="n" />
	</resultMap>
	
	<select id="selectTop3" resultType="Board" resultMap="resultBoardTop3">
		SELECT * FROM WORLD.CITY WHERE ID = #{ID}
